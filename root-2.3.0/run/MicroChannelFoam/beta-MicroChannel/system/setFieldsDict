FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
    volVectorFieldValue U (0.1 0 0)
);

regions
(
    boxToCell
    {
        box (0 4.5e-5 0) (10.0e-5 5.5e-5 0.1e-5);
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
);


